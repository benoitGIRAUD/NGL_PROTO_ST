
(rules PCB NGL_Proto_EURO
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 4271)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 150.0)
    (clear 150.2)
    (clear 75.0 (type smd_to_turn_gap))
    (clear 37.6 (type smd_smd))
  )
  (padstack "Via[0-1]_600:350_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:350_um" "Via[0-1]_600:350_um" default
  )
  (via 
    "Via[0-1]_600:350_um-kicad_default" "Via[0-1]_600:350_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_600:350_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:350_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 150.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
      (length -1.0 0.2)
    )
  )
  (class "kicad_default"
    "-BATT" +3V3 "Net-(C9-Pad1)" "Net-(R2-Pad2)" "Net-(R4-Pad2)" "Net-(R10-Pad2)" "Net-(U4-Pad7)" "Net-(U4-Pad8)"
